{
  "name": "duo-contract",
  "version": "1.0.0",
  "private": true,
  "description": "DUO Contract",
  "author": "FinBook Pte Ltd",
  "scripts": {
    "test": "truffle.cmd test",
    "clean": "rimraf build",
    "start": "ganache-cli --defaultBalanceEther 1000 --gasLimit 7990000 --port 8545",
    "copyAbi": "copyfiles -f .\\build\\contracts\\Custodian.json .\\build\\contracts\\DUO.json .\\build\\contracts\\TokenA.json .\\build\\contracts\\TokenB.json ..\\duo-admin\\src\\static\\",
    "deploy": "truffle.cmd migrate --reset --compile-all",
    "deployKovan": "npm run clean && truffle.cmd migrate --reset --compile-all --network kovan",
    "deployRopsten": "npm run clean && truffle.cmd migrate --reset --compile-all --network ropsten",
    "deployLive": "npm run clean && truffle.cmd migrate --reset --compile-all --network live"
  },
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "ganache-cli": "^6.1.3",
    "rimraf": "^2.6.2",
    "solium": "^1.1.7",
    "truffle": "^4.1.11"
  },
  "dependencies": {
    "web3": "^1.0.0-beta.34"
  }
}
